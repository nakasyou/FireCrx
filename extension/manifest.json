{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "name": "FireCws",
  "manifest_version": 2,
  "version": "0.1.6",
  "content_scripts": [
    {
      "matches": [
        "*://chrome.google.com/webstore/*"
      ],
      "js": [
        "content_scripts/main.js"
      ]
    }
  ],
  "permissions": [
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "applications": {
    "gecko": {
      "id": "nakasyou1103@gmail.com",
      "strict_min_version": "42.0"
    }
  },
  "icons": {
    "512": "./assets/firecrx-512x512.png"
  }
}
